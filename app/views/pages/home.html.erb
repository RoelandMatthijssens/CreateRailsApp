<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title></title>
  </head>
  <body>
    <h3>ready to develop</h3>

    <h4>Active Model Forms</h4>
    <%= form_for @form, url: form_target_path do |f| %>
      <div class="form-group">
        <%= f.label :random_nr %>
        <%= f.text_field :random_nr %>
      </div>
      <div class="form-group">
        <%= f.label :random_letter %>
        <%= f.text_field :random_letter %>
      </div>

      <div class="actions">
        <%= button_tag 'Submit' %>
      </div>
    <% end %>
  </body>
</html>
